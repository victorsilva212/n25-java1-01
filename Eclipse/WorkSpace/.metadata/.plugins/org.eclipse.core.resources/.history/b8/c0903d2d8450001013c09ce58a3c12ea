package br.com.senaisp.bauru.aula07;

public class Exemplo07 {

	public static void main(String[] args) {
		//Erro de sintaxe
		int x=1, y=2;
		if (x==y) {
			System.out.println("Aqui tem erro de sintaxe!");
		}
		if (x<y);{
			System.out.println("X Ã© menor que Y");
		}
	for(int i=0;i<5;i++);{//finalizou o for antes do bloco
		System.out.println("*");
	}

	}
}
